{
  "name": "next-fullstack-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "commit": "cz",
    "console": "tsx ./src/console.ts",
    "typecheck": "tsc --noEmit",
    "format": "yarpm run lint --fix",
    "email:create": "email create --out=./src/server/services/email/templates",
    "email:check": "email check ./src/server/services/email/templates",
    "email:preview": "email preview ./src/server/services/email/templates --no-open --port 2999",
    "prisma:better-auth-generate": "pnpm dlx @better-auth/cli generate --config ./src/server/services/auth.ts",
    "prisma:reset": "yarpm run prisma:migrate:dev reset",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:push": "prisma db push",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "update-all-dependencies": "pnpm dlx npm-check-updates -u",
    "prepare": "is-ci || husky",
    "generate": "yarpm run prisma:generate",
    "postinstall": "yarpm run generate"
  },
